{
  "functions": {
    "api/**/*.[jt]s": { "runtime": "vercel-deno@0.7.8" }
  },
  "routes": [
    {
      "src": "/v1/announces/summary",
      "dest": "api/summary"
    },
    {
      "src": "/v1/announces/list",
      "dest": "api/list"
    },
    {
      "src": "/v1/announces/files",
      "dest": "api/files"
    },
    {
      "src": "/v1/announces/announce",
      "dest": "api/announce"
    }
  ]
}
